{
    "name": "s25viewer-react",
    "version": "0.2.2",
    "description": "S25 viewer written in React + WASM.",
    "main": "index.js",
    "repository": "https://github.com/3c1u/s25",
    "author": "Hikaru Terazono (3c1u)",
    "license": "MIT",
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^4.15.1",
        "@typescript-eslint/parser": "^4.11.1",
        "@wasm-tool/wasm-pack-plugin": "^1.3.1",
        "autoprefixer": "^9.8.6",
        "browserslist": "^4.16.3",
        "eslint": "^7.20.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-config-prettier": "^7.1.0",
        "eslint-import-resolver-parcel": "^1.10.6",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-react": "^7.22.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "file-loader": "^6.2.0",
        "html-loader": "^2.1.2",
        "html-webpack-plugin": "^4.5.0",
        "postcss-modules": "^3.2.2",
        "postcss-normalize": "^9.0.0",
        "prettier": "^2.2.1",
        "rimraf": "^3.0.2",
        "stylus": "^0.54.8",
        "stylus-supremacy": "^2.14.5",
        "ts-loader": "^8.0.17",
        "typescript": "^4.1.5",
        "webpack": "^5.24.4",
        "webpack-cli": "^4.5.0",
        "webpack-dev-server": "^3.11.2"
    },
    "scripts": {
        "serve": "webpack serve",
        "deploy": "webpack --mode production",
        "clean": "rimraf dist",
        "fmt": "yarn fmt:ts && yarn fmt:stylus",
        "fmt:json": "prettier --write \"./**/*.{json,prettierrc,parcelrc,postcssrc}\"",
        "fmt:ts": "prettier --write \"./src/**/*.{ts,tsx}\"",
        "fmt:ts:dry": "prettier --check \"./src/**/*.{ts,tsx}\"",
        "fmt:stylus": "stylus-supremacy format ./src/**/*.styl --replace",
        "fmt:stylus:dry": "stylus-supremacy format ./src/**/*.styl --compare",
        "fmt:dry": "yarn fmt:ts:dry && yarn fmt:stylus:dry",
        "lint": "yarn fmt:dry && eslint --ext .ts,.tsx ./src",
        "lint:fix": "yarn fmt && eslint --ext .ts,.tsx ./src --fix"
    },
    "dependencies": {
        "@material-ui/core": "^4.11.2",
        "@material-ui/icons": "^4.11.2",
        "@types/react": "^17.0.0",
        "@types/react-color": "^3.0.4",
        "@types/react-dom": "^17.0.1",
        "@types/react-redux": "^7.1.14",
        "react": "^17.0.1",
        "react-color": "^2.19.3",
        "react-dom": "^17.0.1",
        "react-redux": "^7.2.2",
        "redux": "^4.0.5",
        "s25-wasm": "../s25-wasm/pkg"
    },
    "browserslist": [
        "> 1.0%",
        "last 2 chrome versions",
        "last 2 firefox versions",
        "last 1 safari version",
        "not IE 11",
        "not IE_Mob 11",
        "Firefox ESR"
    ]
}
